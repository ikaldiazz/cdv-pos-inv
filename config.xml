<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mbutgae.omega" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Omega Invento</name>
    <description>
        Omega Point Of Sale and Inventory Management System
    </description>
    <author email="mbutgaecorp@gmail.com" href="https://gardenexoteak.com">
        Mbutgae Developer Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="phonegap-version" value="cli-6.5.0" />
    <engine name="browser" spec="~4.0.0" />
    <engine name="android" spec="~5.0.0" />
</widget>
